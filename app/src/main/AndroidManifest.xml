<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tu.cellme">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TLendingLibrary"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        android:name="Cellme"
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SellActivity" />
        <activity android:name=".BuyActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".SellSpecActivity" />
        <activity android:name=".NoButtonActivity" />
        <activity android:name=".SellSpec2Activity" />
        <activity android:name=".PriceActivity" />
        <activity android:name=".BuyMobileActivity" />
        <activity android:name=".BuyMobilePartActivity" />
        <activity android:name=".BuyMobileAddressActivity" />
        <activity android:name=".SellAddressActivity" />
        <activity android:name=".RentActivity" />
        <activity android:name=".Buy2Activity" />
    </application>

</manifest>